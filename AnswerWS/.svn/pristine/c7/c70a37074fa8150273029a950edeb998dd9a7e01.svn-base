<!-- install to /usr/axis2c/modules/module/module.xml -->
<module name="module" class="anubis-webservice-axismodule">
    <inflow>
        <handler name="ModuleInHandler" class="anubis-webservice-axismodule">
            <order phase="AnubisWebservicesModuleIn"/>
        </handler>
    </inflow>

    <outflow>
        <handler name="ModuleOutHandler" class="anubis-webservice-axismodule">
            <order phase="AnubisWebservicesModuleOut"/>
        </handler>
    </outflow>
    <Outfaultflow>
        <handler name="ModuleOutFaultHandler" class="axis2_mod_authentication">
            <order phase="AnubisWebservicesModuleOutFault"/>
        </handler>
    </Outfaultflow>
</module>
